<div id="solrExplain-head" >
    {[
      "<%=j path_to_stylesheet 'style' %>",
      "<%= javascript_path 'explain' %>",
      "<%= javascript_path 'highcharts' %>",
      "<!--[if IE]> <%= javascript_path 'excanvas.compiled' %> <![endif]-->"
    ]},
     <%#= stylesheet_link_tag 'style' %>
            <%#= javascript_include_tag :defaults %>
            <%#= javascript_include_tag 'explain' %>
            <%= javascript_include_tag 'highcharts' %> <!--[if IE]> <%= javascript_include_tag 'excanvas.compiled' %> <![endif]-->
</div>
<div id="solrExplain-notice" >
	<p id="notice"> <%= notice %> </p>
</div>
<div id="solrExplain-content">
    <%= yield %>
</div>